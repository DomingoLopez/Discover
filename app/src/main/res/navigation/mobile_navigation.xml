<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.hdrescuer.supportyourdiscoveries.ui.ui.tendencias.LatestPlacesListFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_latest_places" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.hdrescuer.supportyourdiscoveries.ui.ui.photo.MyPlacesListFragment"
        android:label="@string/title_photo"
        tools:layout="@layout/fragment_my_places" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.hdrescuer.supportyourdiscoveries.ui.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>